# Supabase (copy from user-feedback project)
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# App settings
APP_SECRET=change-this-to-random-string
DEBUG=true

# Telegram Bot (create bot via @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_WEBHOOK_URL=https://your-domain.com/api/telegram/webhook

# Admin Telegram Chat ID for bug reports
# Get it by: 1) Send /start to your bot
#            2) Open https://api.telegram.org/bot<TOKEN>/getUpdates
#            3) Find chat.id in the response
ADMIN_TELEGRAM_CHAT_ID=your_admin_chat_id

# DaData (company lookup by INN)
DADATA_API_KEY=your_dadata_api_key

# Sentry (error tracking)
SENTRY_DSN=your_sentry_dsn
SENTRY_ENVIRONMENT=production
